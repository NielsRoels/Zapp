extends ../layout

block content
 nav.app__nav
  div.col-md-6.col-md-offset-3.text-center
   h1 Where would you like to go?
  div.clearfix
 div.choice__body
  div(id="choice__link__1" class="col-md-6 col-ms-6 col-xs-6 choice__body__link choice__body__link--1")
   a(href="choice/route")
    p HOME
  div(id="choice__link__2" class="col-md-6 col-ms-6 col-xs-6 choice__body__link choice__body__link--2") 
   a(href="choice/suggestion")
